{
  "name": "sveltekit-cdk",
  "version": "0.0.1",
  "description": "Tools for deploying SvelteKit to AWS using CDK",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "pnpm build --filter=\"@sveltekit-cdk/*\" --filter=\"sample-site\"",
    "synth": "pnpm cdk synth --filter=\"sample-stack\"",
    "diff": "pnpm cdk diff --filter=\"sample-stack\"",
    "deploy": "pnpm cdk deploy --filter=\"sample-stack\"",
    "bump": "changeset version",
    "pub": "pnpm publish --access public --filter=\"@sveltekit-cdk/*\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juranki/sveltekit-cdk.git"
  },
  "keywords": [],
  "author": "juhani@juranki.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/juranki/sveltekit-cdk/issues"
  },
  "homepage": "https://github.com/juranki/sveltekit-cdk#readme",
  "devDependencies": {
    "@changesets/cli": "^2.17.0"
  }
}
